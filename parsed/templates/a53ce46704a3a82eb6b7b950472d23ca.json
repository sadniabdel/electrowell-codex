{"components/carousel":"<section class=\"home-slideshow\">\r\n\t<div class=\"heroCarousel owl-carousel {{#if theme_settings.home_slide_show_style '===' 'box'}}hero-box{{else}}hero-wide{{/if}}\"\r\n\t\tdata-owl='{\r\n\t\t\t\"nav\": true,\r\n\t\t\t\"dots\": true,\r\n\t\t\t\"items\": 1,\r\n\t\t\t\"autoplay\": true,\r\n\t\t\t\"autoplayTimeout\": {{carousel.swap_frequency}},\r\n\t\t\t\"autoplayHoverPause\": true,\r\n\t\t\t\"loop\": true,\r\n\t\t\t\"responsiveRefreshRate\": 0\r\n\t\t}'>\r\n\t\t{{#each carousel.slides}}\r\n\t\t<div class=\"heroCarousel-slide\">\r\n\t\t\t<a href=\"{{url}}\" target=\"_blank\">\r\n\t\t\t\t<img class=\"heroCarousel-image\" src=\"{{image}}\" alt=\"{{alt_text}}\" title=\"{{alt_text}}\"/>\r\n\t\t\t\t{{#if heading}}\r\n\t\t\t\t\t{{> components/carousel-content show_background=true}}\r\n\t\t\t\t{{else}}\r\n\t\t\t\t\t{{#if text}}\r\n\t\t\t\t\t\t{{> components/carousel-content show_background=true}}\r\n\t\t\t\t\t{{else}}\r\n\t\t\t\t\t\t{{#if button_text}}\r\n\t\t\t\t\t\t\t{{> components/carousel-content show_background=false}}\r\n\t\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t{{/if}}\r\n\t\t\t</a>\r\n\t\t</div>\r\n\t\t{{/each}}\r\n\t</div>\r\n</section>\r\n","components/carousel-content":"<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\r\n    <h1 class=\"heroCarousel-title\">{{heading}}</h1>\r\n    <p class=\"heroCarousel-description\">{{text}}</p>\r\n    {{#if button_text}}\r\n        <span class=\"heroCarousel-action btn btn-primary btn-lg\">{{button_text}}</span>\r\n    {{/if}}\r\n</div>\r\n"}
