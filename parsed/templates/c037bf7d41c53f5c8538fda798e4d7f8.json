{"components/common/login-for-pricing":"<p translate>Log in for pricing</p>\n","components/products/card-top-seller":"<article class=\"card {{#if alternate}}card--alternate{{/if}}\">\r\n    <figure class=\"card-figure\">\r\n        {{#if theme_settings.show_product_brand}}\r\n            {{#if brand.name}}\r\n                <p class=\"prod-brand\" data-test-info-type=\"brandName\">{{brand.name}}</p>\r\n            {{/if}}\r\n        {{/if}}\r\n        <h4 class=\"prod-name\">\r\n            <a href=\"{{url}}\">{{name}}</a>\r\n        </h4>\r\n        <div class=\"prod-image\">\r\n            <a href=\"{{url}}\">\r\n                <img class=\"card-image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'productgallery_size' (cdn theme_settings.default_image_product)}}\"\r\n                {{#if theme_settings.halothemes_images_swap}}\r\n                {{#if images.length '>=' 2}}\r\n                {{#each images}}\r\n                {{#if @index '==' 1}}\r\n                data-src-swap=\"{{getImage this 'productgallery_size' (cdn theme_settings.default_image_product)}}\"\r\n                {{/if}}\r\n                {{/each}}\r\n                {{/if}}\r\n                {{/if}}\r\n                alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\r\n            </a>\r\n            <a href=\"/wishlist.php?action=add&product_id={{id}}\" class=\"btnWL hidden-xs hidden-sm hidden-md\" title=\"{{lang 'account.wishlists.add_item'}}\">Wishlist</a>\r\n            <div class=\"actions\">\r\n                {{#unless hide_product_quick_view}}\r\n                    {{#if theme_settings.show_product_quick_view}}\r\n                        <a href=\"#\" class=\"btnQV hidden-xs hidden-sm hidden-md quickview\" data-product-id=\"{{id}}\">{{lang 'products.quick_view'}}</a>\r\n                    {{/if}}\r\n                {{/unless}}\r\n            </div>\r\n            {{#if price.saved}}\r\n            <div class=\"sale-badge\">{{lang 'products.sale_badge'}}</div>\r\n            {{/if}}\r\n        </div>\r\n        <figcaption class=\"prod-desc\">\r\n            {{#if theme_settings.show_product_brand}}\r\n                {{#if brand.name}}\r\n                    <p class=\"prod-brand\" data-test-info-type=\"brandName\">{{brand.name}}</p>\r\n                {{/if}}\r\n            {{/if}}\r\n            <h4 class=\"prod-name\">\r\n                <a href=\"{{url}}\" data-product-id=\"{{id}}\">{{name}}</a>\r\n            </h4>\r\n\r\n            <div class=\"star-rating\" data-test-info-type=\"productRating\">\r\n                <div class=\"rating--small\">\r\n                    {{> components/products/ratings rating=rating}}\r\n                </div>\r\n            </div>\r\n\r\n            <div class=\"prod-price\" data-test-info-type=\"price\">\r\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                    {{> components/products/price price=price customer=customer}}\r\n                {{else}}\r\n                    {{> components/common/login-for-pricing}}\r\n                {{/or}}\r\n            </div>\r\n            {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n            {{#if show_cart_action}}\r\n                <div class=\"addto-cart\">\r\n                {{#if has_options}}\r\n                    <a href=\"{{url}}\" class=\"btn btn-alt btnATC\" data-product-id=\"{{id}}\">{{lang 'products.choose_options'}}<i class=\"fa fa-chevron-right\"></i></a>\r\n                {{/if}}\r\n                {{#if pre_order}}\r\n                    <a href=\"{{url}}\" class=\"btn btn-alt btnATC\">{{lang 'products.pre_order'}}<i class=\"fa fa-chevron-right\"></i></a>\r\n                {{/if}}\r\n                {{#if add_to_cart_url }}\r\n                    <a href=\"{{add_to_cart_url}}\" class=\"btn btn-alt btnATC\">{{lang 'products.add_to_cart'}}<i class=\"fa fa-chevron-right\"></i></a>\r\n                {{/if}}\r\n                {{#if out_of_stock_message }}\r\n                    <a href=\"{{url}}\" class=\"btn btn-alt btnATC\" data-product-id=\"{{id}}\">{{out_of_stock_message}}</a>\r\n                {{/if}}\r\n                </div>\r\n            {{/if}}\r\n            {{/or}}\r\n        </figcaption>\r\n    </figure>\r\n</article>\n","components/products/price":"{{#if price.with_tax}}\r\n    <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n        {{#if price.rrp_with_tax}}\r\n            <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n        {{/if}}\r\n        <span data-product-price-with-tax class=\"price\">{{price.with_tax.formatted}}</span>\r\n        {{#if price.without_tax}}\r\n            {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n        {{/if}}\r\n    </div>\r\n{{/if}}\r\n\r\n{{#if price.without_tax}}\r\n    <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n        {{#if price.rrp_without_tax}}\r\n            <span data-product-rrp-without-tax class=\"price price--rrp\">{{price.rrp_without_tax.formatted}}</span>\r\n        {{/if}}\r\n        {{#if schema_org}}\r\n            <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n            <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n        {{/if}}\r\n        <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n        {{#if price.with_tax}}\r\n            <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n        {{/if}}\r\n    </div>\r\n{{/if}}\r\n\r\n{{#if page_type '===' 'product'}}\r\n    {{#if price.saved}}\r\n        <div class=\"price-section price-section--saving\">\r\n            <span class=\"price\">\r\n                {{lang 'products.you_save' amount=price.saved.formatted}}\r\n            </span>\r\n        </div>\r\n    {{/if}}\r\n{{/if}}\r\n","components/products/ratings":"{{#for 1 5}}\r\n    {{#if ../this.rating '>=' $index}}\r\n        <i class=\"fa fa-star active\"></i>\r\n    {{else}}\r\n        <i class=\"fa fa-star\"></i>\r\n    {{/if}}\r\n{{/for}}\r\n{{{snippet 'product_rating'}}}\r\n","components/products/side-carousel":"<section class=\"productCarousel owl-carousel\"\r\n    data-owl='{\r\n        \"nav\": true,\r\n        \"dots\": false,\r\n        \"items\": {{columns}},\r\n        \"margin\": 0,\r\n        \"slideBy\": {{columns}},\r\n        \"responsive\": {\r\n         \"0\": {\r\n            \"items\": 1,\r\n            \"slideBy\": 1\r\n         },\r\n         \"768\": {\r\n            \"items\": 1,\r\n            \"slideBy\": 1\r\n         },\r\n         \"992\": {\r\n            \"items\": 1,\r\n            \"slideBy\": 1\r\n         },\r\n         \"1200\": {\r\n            \"items\": {{columns}},\r\n            \"slideBy\": {{columns}}\r\n         }\r\n      },\r\n      \"responsiveRefreshRate\": 0\r\n    }'>\r\n    {{#each products}}\r\n    <div class=\"prod-item\">\r\n        {{> components/products/card-top-seller theme_settings=../theme_settings customer=../customer}}\r\n    </div>\r\n    {{/each}}\r\n</section>\r\n"}
